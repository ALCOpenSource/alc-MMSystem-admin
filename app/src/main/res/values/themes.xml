<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AlcMMSystemadmin" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="regularB">
        <item name="android:fontFamily">@font/mukta_regular</item>
        <item name="android:textColor">@color/black2</item>
        <item name="android:textSize">@dimen/font_small</item>
    </style>

    <style name="mediumMukta">
        <item name="android:fontFamily">@font/mukta_bold</item>
        <item name="android:textColor">@color/black2</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>

    <style name="mediumRoboto">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">@color/black2</item>
        <item name="android:textSize">@dimen/font_regular</item>
    </style>


    <style name="CircleImage">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ChipProfile" parent="Widget.MaterialComponents.Chip.Action">
        <item name="chipStrokeColor">@color/chip_stroke</item>
        <item name="chipBackgroundColor">@color/green11</item>
        <item name="chipCornerRadius">5dp</item>
        <item name="chipStrokeWidth">1dp</item>
        <item name="android:textColor">@color/pry2</item>
        <item name="android:textSize">@dimen/font_small</item>
        <item name="android:textAlignment">center</item>
    </style>


</resources>