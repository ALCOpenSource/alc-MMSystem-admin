<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="com.peculiaruc.alc_mmsystem_admin.ui.approvalRequests.ApprovalRequestsViewModel" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mentor_request"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="581dp"
            android:background="#F2F2F2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/mentorManagerTextView"
                android:layout_width="wrap_content"
                android:layout_height="27dp"
                android:layout_marginStart="121dp"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="14dp"
                android:layout_marginBottom="55dp"
                android:fontFamily="@font/mukta_medium"
                android:text="@string/mentor_manager_requests"
                android:textAlignment="textStart"
                android:textColor="#333333"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/number1TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="121dp"
                android:layout_marginTop="46dp"
                android:layout_marginEnd="156dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/mukta_extrabold"
                android:text="@string/_287"
                android:textAlignment="center"
                android:textColor="#141414"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:lineHeight="39.89dp" />

            <Button
                android:id="@+id/view1Button"
                android:layout_width="65dp"
                android:layout_height="40dp"
                android:layout_marginStart="248dp"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="13dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/mukta_regular"
                android:onClick="@{()-> viewModel.onClickMentorMangerRequest()}"
                android:text="@string/view"
                android:textAllCaps="false"
                android:textColor="#F2F2F2"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="94dp"
                android:layout_height="75dp"
                android:layout_marginStart="19dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="18dp"
                app:circularflow_defaultRadius="10dp"
                app:circularflow_radiusInDP="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/mentorManagerTextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/mm_requests_icon" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mentor_requests"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="456dp"
            android:background="#F2F2F2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mentor_request">

            <TextView
                android:id="@+id/mentorRequestsTextView"
                android:layout_width="wrap_content"
                android:layout_height="27dp"
                android:layout_marginStart="121dp"
                android:layout_marginTop="28dp"
                android:layout_marginBottom="55dp"
                android:fontFamily="@font/mukta_medium"
                android:text="Mentor Requests"
                android:textAlignment="textStart"
                android:textColor="#333333"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/number2TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="121dp"
                android:layout_marginTop="46dp"
                android:layout_marginEnd="156dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/mukta_extrabold"
                android:text="160"
                android:textAlignment="center"
                android:textColor="#141414"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:lineHeight="39.89dp" />

            <Button
                android:id="@+id/view2Button"
                android:layout_width="65dp"
                android:layout_height="40dp"
                android:layout_marginStart="248dp"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="13dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/mukta_regular"
                android:onClick="@{()-> viewModel.onClickMentorRequest()}"
                android:text="@string/view"
                android:textAllCaps="false"
                android:textColor="#F2F2F2"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="94dp"
                android:layout_height="75dp"
                android:layout_marginStart="19dp"
                android:layout_marginTop="17dp"
                android:layout_marginBottom="18dp"
                app:circularflow_defaultRadius="10dp"
                app:circularflow_radiusInDP="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/mentor_request_icon" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame533"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="331dp"
            android:background="#F2F2F2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mentor_requests">

            <TextView
                android:id="@+id/programRequestsTextView"
                android:layout_width="wrap_content"
                android:layout_height="27dp"
                android:layout_marginStart="121dp"
                android:layout_marginTop="28dp"
                android:layout_marginBottom="55dp"
                android:fontFamily="@font/mukta_medium"
                android:text="Program Requests"
                android:textAlignment="textStart"
                android:textColor="#333333"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/number3TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="121dp"
                android:layout_marginTop="46dp"
                android:layout_marginEnd="156dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/mukta_extrabold"
                android:text="160"
                android:textAlignment="center"
                android:textColor="#141414"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:lineHeight="39.89dp" />

            <Button
                android:id="@+id/view3Button"
                android:layout_width="65dp"
                android:layout_height="40dp"
                android:layout_marginStart="248dp"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="13dp"
                android:layout_marginBottom="15dp"
                android:fontFamily="@font/mukta_regular"
                android:onClick="@{()-> viewModel.onClickProgramRequest()}"
                android:text="@string/view"
                android:textAllCaps="false"
                android:textColor="#F2F2F2"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="94dp"
                android:layout_height="75dp"
                android:layout_marginStart="19dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="18dp"
                app:circularflow_defaultRadius="10dp"
                app:circularflow_radiusInDP="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/programRequestsTextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/program_requests_icon" />

        </androidx.constraintlayout.widget.ConstraintLayout>


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/addMentorFab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:clickable="true"
            android:focusable="true"
            android:onClick="@{()-> viewModel.onClickAdd()}"
            android:src="@drawable/ic_add_sign"
            app:backgroundTint="@color/mms_pry_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/ic_add_sign"
            app:tint="@color/white" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>